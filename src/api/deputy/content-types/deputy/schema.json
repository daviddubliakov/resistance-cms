{
  "kind": "collectionType",
  "collectionName": "deputies",
  "info": {
    "singularName": "deputy",
    "pluralName": "deputies",
    "displayName": "Депутат"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "firstName": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "lastName": {
      "type": "string",
      "required": true
    },
    "party": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::party.party",
      "inversedBy": "deputats"
    },
    "fraction": {
      "type": "string",
      "default": "Позафракційний"
    },
    "placeOfEmployment": {
      "type": "string"
    },
    "isCorrupt": {
      "type": "boolean",
      "default": false
    },
    "photo": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images"
      ]
    },
    "shames": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::shame.shame",
      "inversedBy": "deputats"
    },
    "relatedBusinessess": {
      "type": "component",
      "component": "related-businessess.related-businessess",
      "repeatable": true
    },
    "otherIncomes": {
      "type": "component",
      "component": "other-incomes.other-incomes",
      "repeatable": true,
      "required": false
    }
  }
}
